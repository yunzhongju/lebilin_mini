.store-manager {
  box-sizing: border-box;
  padding: 0 42rpx;
  padding-top: 126rpx;
  position: relative;
}
.store-manager .audit-status {
  height: 126rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  background-color: #E62329;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}
.store-manager .audit-status .title {
  font-size: 30rpx;
  letter-spacing: 3rpx;
  color: #ffffff;
}
.store-manager .box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.store-manager .box.open-type-btn {
  padding: 0;
  width: 100%;
  border: 0;
  background-color: #fff;
  text-align: start;
}
.store-manager .box.open-type-btn:after {
  content: "";
  border: none;
}
.store-manager .box .title {
  font-size: 30rpx;
  letter-spacing: 3rpx;
  line-height: 100rpx;
}
.store-manager .box .content {
  width: 482rpx;
  height: 55rpx;
  background-color: #efefef;
  font-size: 30rpx;
  line-height: 55rpx;
  border-radius: 10rpx;
  padding-left: 24rpx;
}
.store-manager .supplier-protocol {
  margin-top: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.store-manager .supplier-protocol .choose {
  width: 25rpx;
  height: 25rpx;
  margin-right: 12rpx;
}
.store-manager .supplier-protocol .title {
  font-size: 25rpx;
  line-height: 80rpx;
  letter-spacing: 3rpx;
}
.store-manager .submit-btn {
  width: 450rpx;
  height: 60rpx;
  background-color: #ff2f29;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
  letter-spacing: 3rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.store-manager .submit-btn:after {
  content: "";
  border: none;
}

