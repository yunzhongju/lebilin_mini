<view class="union"><view class="tab-title"><block wx:for="{{titleArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeActiveIndex',[index]]]]]}}" class="item-box" bindtap="__e"><text class="{{['item',(index==activeIndex)?'active':'']}}">{{item}}</text><block wx:if="{{index==activeIndex}}"><text class="bottom-line"></text></block></view></block></view><view class="tab-content"><block wx:for="{{$root.l1}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['pageToMerchanInfo',['$0'],[[['merchanArr','',index]]]]]]]}}" class="content-item" bindtap="__e"><view class="left-pic"><image class="img" lazy-load="true" src="{{item.$orig.thumbnail}}" mode></image></view><view class="right-msg"><view class="top-connect"><view class="union-name"><image class="danghui" src="/pagesC/static/imgs/danghui.png" mode></image><text class="name">{{item.$orig.supplierName}}</text></view><image class="call-union" src="/pagesC/static/imgs/call_union.png" mode data-event-opts="{{[['tap',[['makePhone',['$0'],[[['merchanArr','',index,'phonenum']]]]]]]}}" catchtap="__e"></image></view><view class="score-box"><view data-event-opts="{{[['tap',[['pageToMerchanInfo',['$0'],[[['merchanArr','',index]]]]]]]}}" class="left-scre" bindtap="__e"><block wx:for="{{item.$orig.score}}" wx:for-item="q" wx:for-index="w" wx:key="w"><image src="/static/service_icon/star_icon.png" mode></image></block><block wx:for="{{5-item.$orig.score}}" wx:for-item="q" wx:for-index="w" wx:key="w"><image src="/static/service_icon/star_icon_false.png" mode></image></block></view><text class="right-distance">{{item.$orig.distance}}</text></view><view class="address-box"><image class="icon" src="/pagesC/static/imgs/address_green.png" mode></image><text class="address">{{item.$orig.detailAddress}}</text></view><view class="service-box"><block wx:for="{{item.l0}}" wx:for-item="i" wx:for-index="v" wx:key="v"><view class="service-item"><block wx:if="{{i.g0}}"><image class="icon-item" src="{{i.$orig.iconpath[0]}}" mode></image></block><block wx:else><image class="icon-item" src="{{i.$orig.iconpath[1]}}" mode></image></block></view></block></view></view></view></block></view></view>