.document-detail .dynamic-detail {
  box-sizing: border-box;
  padding: 20rpx;
  border-bottom: 7rpx solid #efefef;
}
.document-detail .dynamic-detail .font24 {
  font-size: 25rpx;
  line-height: 40rpx;
  width: 100%;
  overflow: hidden;
  text-align: justify;
}
.document-detail .dynamic-detail .font24 .imgsty {
  max-width: 100%;
  margin: 6rpx auto;
  border-radius: 10rpx;
}
.document-detail .dynamic-detail .top-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.document-detail .dynamic-detail .top-title .title {
  font-size: 30rpx;
  line-height: 60rpx;
}
.document-detail .dynamic-detail .top-title .title-detail {
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 22rpx;
  color: #595757;
  line-height: 40rpx;
  margin-bottom: 10rpx;
}
.document-detail .dynamic-detail .dynamic-img {
  width: 100%;
  vertical-align: middle;
  margin: 20rpx 0;
}
.document-detail .dynamic-detail .dynamic-text {
  font-size: 26rpx;
  line-height: 40rpx;
  text-indent: 48rpx;
}
.document-detail .fujian-box {
  box-sizing: border-box;
  padding: 20rpx;
}
.document-detail .fujian-box .top-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-bottom: 8rpx;
}
.document-detail .fujian-box .top-title .img {
  width: 32rpx;
  height: 36rpx;
  margin-right: 27rpx;
}
.document-detail .fujian-box .top-title .title {
  font-size: 25rpx;
  letter-spacing: 3rpx;
}
.document-detail .fujian-box .item-box .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.document-detail .fujian-box .item-box .item .imgs {
  width: 38rpx;
  height: 47rpx;
  margin: 8rpx 0;
  margin-right: 21rpx;
}
.document-detail .fujian-box .item-box .item .title {
  font-size: 25rpx;
  line-height: 30rpx;
  letter-spacing: 3rpx;
  color: #396695;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

