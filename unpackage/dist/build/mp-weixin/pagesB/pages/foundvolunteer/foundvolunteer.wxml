<view class="found-volunteer"><view class="top-type"><block wx:for="{{typeArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['changeCheck',[index]]]]]}}" class="{{[(item.check)?'active':'']}}" bindtap="__e">{{item.name}}</text></block></view><view class="volunteer-list"><block wx:if="{{personArr.length==0}}"><view class="no-volunteer">暂时没有相关人员哦~</view></block><view class="volunteers"><block wx:for="{{personArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="volunteer-item"><image class="left" src="{{item.headerUrl}}" mode></image><view class="right"><view class="right-top"><text>{{item.personname}}</text><block wx:if="{{item.isPartyMember}}"><image src="/pagesB/static/imgs/member.png" mode></image></block></view><view class="right-bottom"><block wx:for="{{item.skills}}" wx:for-item="s" wx:for-index="i" wx:key="i"><text class="type-item">{{s}}</text></block></view><text class="intruduce">{{item.personalExplanation||'无'}}</text></view><image style="width:40rpx;height:40rpx;margin:0 20rpx 0 10rpx;align-self:flex-start;" src="/pagesB/static/imgs/phone.png" mode data-event-opts="{{[['tap',[['makePhoneCall',['$0'],[[['personArr','',index,'personpho']]]]]]]}}" bindtap="__e"></image></view><view class="split-line"></view></view></block></view></view></view>