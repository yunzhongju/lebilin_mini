<view class="project-detail"><view class="project-title">{{''+projectName+''}}</view><view class="project-progress"><block wx:for="{{detailArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="progress-item"><text class="right"><block wx:if="{{item&&item.startTime}}"><text>{{item.startTime}}</text></block><block wx:if="{{item&&item.endTime}}"><text>{{"-"+item.endTime}}</text></block></text><view class="middle"><text class="circle"></text><block wx:if="{{index!=detailArr.length-1}}"><text class="line" style="{{'height:'+(40+item.projectFileList.length*60+'rpx')+';'}}"></text></block></view><view class="left"><text class="left-title">{{item.nodeName}}</text><block wx:for="{{item.projectFileList}}" wx:for-item="v" wx:for-index="i" wx:key="i"><text data-event-opts="{{[['tap',[['pageToDocumentDetail',['$0'],[[['detailArr','',index],['projectFileList','',i,'fileId']]]]]]]}}" class="left-item" bindtap="__e">{{v.fileName}}</text></block></view></view></block></view></view>