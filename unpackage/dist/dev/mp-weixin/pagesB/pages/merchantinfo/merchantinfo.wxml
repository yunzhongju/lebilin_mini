<view class="merchant-info"><image class="merchant-img" src="{{item.thumbnail}}" mode></image><view class="name-score"><text class="name">{{item.supplierName}}</text><view class="score"><block wx:for="{{item.score}}" wx:for-item="i" wx:for-index="v" wx:key="v"><image class="score-items" src="/static/service_icon/star_icon.png" mode="widthFix"></image></block><block wx:for="{{5-item.score}}" wx:for-item="e" wx:for-index="m" wx:key="m"><image class="score-items" src="/static/service_icon/star_icon_false.png" mode="widthFix"></image></block><text class="num">{{item.score}}</text></view></view><view class="merchant-service"><view class="address"><image class="address-img" src="/static/service_icon/address.png" mode></image><text class="title">{{item.detailAddress}}</text></view><view data-event-opts="{{[['tap',[['makeCall',['$0'],['item.phonenum']]]]]}}" class="phone" bindtap="__e"><image class="phone-img" src="/static/service_icon/phone.png" mode></image><text class="title">{{item.phonenum}}</text></view><view class="services"><block wx:for="{{serviceTag}}" wx:for-item="z" wx:for-index="y" wx:key="y"><view class="item"><image class="service-img" src="{{z.pic}}" mode></image><text class="title">{{z.name}}</text></view></block></view></view><view class="shop-provide"><view class="title">本店提供</view><view class="service-box"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsList','',index,'productId']]]]]]]}}" class="youxuan-item" bindtap="__e"><view class="tag-box"><block wx:for="{{item.lables}}" wx:for-item="i" wx:for-index="v" wx:key="v"><text class="tag-items">{{i}}</text></block></view><image class="goods-img" src="{{state.baseUrl+item.picture}}" mode></image><view class="price-box"><text class="goods-name">{{item.goodsName}}</text><text class="goods-depict">{{item.depict}}</text><view class="price"><block wx:if="{{item.integral}}"><text style="color:#3bb81e;">{{item.integral+"分+"}}</text></block><block wx:if="{{item.sellPrice}}"><text style="color:#e62329;">{{"￥"+item.sellPrice+"元/斤"}}</text></block></view></view></view></block></view></view><button class="share-btn" type="default"><image class="share-img" src="/static/service_icon/share_float.png" mode></image></button></view>