<view class="mine-box"><view class="{{['top-personal',(state.userInfo.isVolunteer&&state.userInfo.serviceLevel==3)?'bggold':'',(state.userInfo.isVolunteer&&state.userInfo.serviceLevel==2)?'bgsilver':'',(state.userInfo.isVolunteer&&state.userInfo.serviceLevel==1)?'bgcopper':'',(state.userInfo&&!state.userInfo.isVolunteer)?'bgnot':'']}}"><view class="top"><view class="personal-info"><image class="left-avatar" src="{{state.userInfo.headerUrl}}" mode></image><block wx:if="{{state.userInfo.headerUrl}}"><text class="nickname">{{state.userInfo.nickName}}</text></block><block wx:if="{{state.userInfo.isPartyMember}}"><image class="is-member" src="/static/mine_icon/part_member.png" mode></image></block><block wx:if="{{!state.userInfo.headerUrl}}"><button class="login-btn" type="default" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getInfo',['$event']]]]]}}" bindgetuserinfo="__e">立即登录</button></block></view><view class="message"><view data-event-opts="{{[['tap',[['pageToMineInfo',['$event']]]]]}}" class="message-item" bindtap="__e"><image class="icon2" src="/static/mine_icon/setting.png" mode></image></view><view data-event-opts="{{[['tap',[['pageToMyMessage',['$event']]]]]}}" class="message-item" bindtap="__e"><image class="icon" src="/static/mine_icon/message.png" mode></image><block wx:if="{{hasMessage}}"><text class="sign-point"></text></block></view></view></view><view class="money-intergral"><view class="intergral"><image class="img1" src="/static/mine_icon/intergral.png" mode></image><text class="number">{{myIntergral}}</text></view><view data-event-opts="{{[['tap',[['pageToSortSign',['$event']]]]]}}" class="sign" bindtap="__e"><image class="img2" src="/static/mine_icon/sign.png" mode></image><text class="number">签到</text></view><view class="active-time"><image class="img3" src="/static/mine_icon/active_time.png" mode></image><text class="number">{{state.userInfo.serviceCount}}</text></view><view data-event-opts="{{[['tap',[['pageToWithdrawals',['$event']]]]]}}" class="money" bindtap="__e"><image class="img4" src="/static/mine_icon/money.png" mode></image><text class="number">{{"￥"+(state.userInfo.balance||'0.00')}}</text></view></view><view class="msg-bottom"><block wx:if="{{state.userInfo.isVolunteer}}"><view class="become-volunteer"><block wx:if="{{state.userInfo.serviceLevel==3}}"><image class="score" src="/static/mine_icon/gold.png" mode></image></block><block wx:if="{{state.userInfo.serviceLevel==2}}"><image class="score" src="/static/mine_icon/silver.png" mode></image></block><block wx:if="{{state.userInfo.serviceLevel==1}}"><image class="score" src="/static/mine_icon/copper.png" mode></image></block><text class="title">{{"您已是"+(state.userInfo.serviceLevel==1?'铜牌':state.userInfo.serviceLevel==2?'银牌':'金牌')+"志愿者"}}</text></view></block><block wx:if="{{state.userInfo&&!state.userInfo.isVolunteer}}"><view class="apply-volunteer"><text class="title">您还不是志愿者</text><text data-event-opts="{{[['tap',[['pageToApplyVolunteer',['$event']]]]]}}" class="btn" bindtap="__e">加入</text></view></block></view></view><view class="my-order card"><view class="top"><text class="title">我的订单</text><view data-event-opts="{{[['tap',[['pageToMyOrder',[0]]]]]}}" class="all-order" bindtap="__e"><text>全部订单</text><image src="/static/mine_icon/arrow.png" mode></image></view></view><view class="items-box"><view data-event-opts="{{[['tap',[['pageToMyOrder',[1]]]]]}}" class="item" bindtap="__e"><image class="item-img1" src="/static/mine_icon/need_pay.png" mode></image><text class="title">待支付</text></view><view data-event-opts="{{[['tap',[['pageToMyOrder',[2]]]]]}}" class="item" bindtap="__e"><image class="item-img2" src="/static/mine_icon/need-recive.png" mode></image><text class="title">待收货</text></view><view data-event-opts="{{[['tap',[['pageToMyOrder',[4]]]]]}}" class="item" bindtap="__e"><image class="item-img3" src="/static/mine_icon/compoment.png" mode></image><text class="title">已完成</text></view><view data-event-opts="{{[['tap',[['pageToMyOrder',[2]]]]]}}" class="item" bindtap="__e"><image class="item-img4" src="/static/mine_icon/shouhou.png" mode></image><text class="title">售后/退款</text></view></view></view><view class="navigation card"><view class="items-box"><view data-event-opts="{{[['tap',[['pageToVolunteerInfo',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img" src="/static/mine_icon/volunteer_icon.png" mode></image><text class="title">志愿者证书</text></view><view data-event-opts="{{[['tap',[['pageToSupplier',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img" src="/static/mine_icon/supplier_icon.png" mode></image><text class="title">供应商</text></view><view data-event-opts="{{[['tap',[['pageToShopManage',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img" src="/static/mine_icon/store_icon.png" mode></image><text class="title">店铺管理</text></view><view data-event-opts="{{[['tap',[['pageToStoreManage',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img" src="/static/mine_icon/boss_icon.png" mode></image><text class="title">店长入驻</text></view></view><view class="items-box2"><view data-event-opts="{{[['tap',[['pageToPlayWithIntegral',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img" src="/static/mine_icon/play_intergral_icon.png" mode></image><text class="title">玩转积分</text></view><view data-event-opts="{{[['tap',[['pageToIntergral',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img2" src="/static/mine_icon/intergral_shop_icon.png" mode></image><text class="title">积分商城</text></view><view data-event-opts="{{[['tap',[['pageToMyBill',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img3" src="/static/mine_icon/intergral_list_icon.png" mode></image><text class="title">账单</text></view></view></view></view>