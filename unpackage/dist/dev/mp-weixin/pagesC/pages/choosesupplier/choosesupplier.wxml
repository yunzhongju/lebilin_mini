<view class="choose-supplier"><view class="search-box"><input type="text" focus="{{isFocus}}" value="{{searchContent}}" placeholder="请输入关键字" data-event-opts="{{[['input',[['__set_model',['','searchContent','$event',[]]]]]]}}" bindinput="__e"/><text>搜索</text><image class="search-icon" src="/pagesC/static/imgs/search_icon.png" mode></image><block wx:if="{{searchContent}}"><image class="close-icon" src="/pagesC/static/imgs/close.png" mode data-event-opts="{{[['tap',[['clearContent',['$event']]]]]}}" bindtap="__e"></image></block></view><view class="list-box"><block wx:for="{{supplierList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-item"><view class="left"><text class="top">{{item.supplierName}}</text><text class="middle">{{"联系人："+item.orgContactor}}</text><text class="km">{{"送货范围："+(item.distance+item.unit)}}</text><text class="bottom">{{"地址："+item.detailAddress}}</text></view><view data-event-opts="{{[['tap',[['pageToGoodsShelves',['$0'],[[['supplierList','',index,'supplierId']]]]]]]}}" class="right" bindtap="__e">商品上架</view></view></block></view></view>