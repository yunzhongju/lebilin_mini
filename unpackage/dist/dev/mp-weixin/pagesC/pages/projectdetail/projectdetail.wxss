page {
  background-color: #EFEFEF;
}
.project-detail .project-title {
  font-size: 34rpx;
  font-weight: bold;
  line-height: 108rpx;
  text-align: center;
  box-sizing: border-box;
  padding: 0 80rpx;
}
.project-detail .project-progress {
  box-sizing: border-box;
  width: 680rpx;
  margin: 0 auto;
  padding: 64rpx 56rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
}
.project-detail .project-progress .progress-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.project-detail .project-progress .progress-item .right {
  width: 133rpx;
  font-size: 24rpx;
  color: #ff2f29;
}
.project-detail .project-progress .progress-item .middle {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.project-detail .project-progress .progress-item .middle .circle {
  width: 24rpx;
  height: 24rpx;
  background-color: #ff2f29;
  border-radius: 50%;
}
.project-detail .project-progress .progress-item .middle .line {
  width: 3rpx;
  background-color: #ff2f29;
  margin: 10rpx 0;
}
.project-detail .project-progress .progress-item .left {
  width: 313rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.project-detail .project-progress .progress-item .left .left-title {
  font-size: 30rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 12rpx;
}
.project-detail .project-progress .progress-item .left .left-item {
  font-size: 24rpx;
  line-height: 60rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

