<view class="infomation-disclosure"><block wx:for="{{ProjectArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="card information-item"><view data-event-opts="{{[['tap',[['pageToProjectDetail',['$0','$1'],[[['ProjectArr','',index,'projectId']],[['ProjectArr','',index,'projectName']]]]]]]}}" class="img-box" bindtap="__e"><image class="img" src="{{item.thumbnail}}" mode></image><text class="explain">{{item.projectName}}</text></view><view class="progress-box"><block wx:for="{{item.projectFileList}}" wx:for-item="v" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['pageToDocumentDetail',['$0'],[[['ProjectArr','',index],['projectFileList','',i,'fileId']]]]]]]}}" class="progress-item" bindtap="__e"><text class="title">{{v.fileName}}</text><view class="time-box"><text class="time">{{v.uploadTime}}</text><image class="go" src="/pagesC/static/imgs/right_arrow.png" mode></image></view></view></block></view></view></block></view>