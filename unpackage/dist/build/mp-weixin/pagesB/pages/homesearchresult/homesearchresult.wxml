<view class="home-search-box"><view class="top-key"><image class="search-icon" src="/pagesB/static/imgs/search_icon.png" mode></image><input class="input-box" confirm-type="search" type="text" value="{{searchResult}}" placeholder="请输入关键字" data-event-opts="{{[['confirm',[['subSearchResult',['$event']]]],['input',[['__set_model',['','searchResult','$event',[]]]]]]}}" bindconfirm="__e" bindinput="__e"/><text data-event-opts="{{[['tap',[['clearContent',['$event']]]]]}}" class="clear" bindtap="__e">清空</text></view><view class="tab-title"><block wx:for="{{titleArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeIndex',[index]]]]]}}" class="title-item" bindtap="__e"><text class="item-title">{{item}}</text><block wx:if="{{currentIndex==index}}"><text class="item-underline"></text></block></view></block></view><view class="tab-content"><block wx:if="{{currentIndex==0}}"><view class="activity-list-box"><block wx:if="{{activityArr.length==0}}"><view style="font-size:30rpx;text-align:center;line-height:100rpx;">暂时没有相关活动哦~</view></block><block wx:else><view class="activity"><view class="card padding-item"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item-box"><view class="acive-item"><view data-event-opts="{{[['tap',[['pageToDetail',['$0'],[[['activityArr','',index,'activityId']]]]]]]}}" class="img-box" bindtap="__e"><image class="active-item-img" lazy-load="{{true}}" src="{{item.$orig.acpicUrl}}" mode></image><view class="sign-box"><block wx:for="{{item.$orig.theme}}" wx:for-item="items" wx:for-index="i" wx:key="i"><text>{{items}}</text></block></view></view><view class="active-detail"><view class="detail-left"><text class="title">{{item.$orig.activityName}}</text><text class="time">{{"活动日期:"+item.g0+"―"+item.g1}}</text><text class="address">{{item.$orig.addr}}</text></view><view class="detail-right"><view class="baoming-status"><image src="/static/home_icon/baoming_icon.png"