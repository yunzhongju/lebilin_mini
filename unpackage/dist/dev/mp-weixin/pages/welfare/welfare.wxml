<view class="super-shop-box"><uni-nav-bar vue-id="dfd26234-1" fixed="{{true}}" status-bar="{{true}}" shadow="{{false}}" bind:__l="__l" vue-slots="{{['default','left']}}"><view style="position:relative;"><image class="search-icon" src="/static/home_icon/search_icon.png"></image><input class="search-color" confirm-type="search" type="text" placeholder="搜索" placeholder-style="color: #bababa;" data-event-opts="{{[['confirm',[['consoleValue',['$event']]]],['input',[['__set_model',['','searchGoods','$event',[]]]]]]}}" value="{{searchGoods}}" bindconfirm="__e" bindinput="__e"/></view><view class="text-white" slot="left">福利</view></uni-nav-bar><view class="store-title"><view class="left"><image class="img" src="{{storeInfo.thumbnail}}" mode></image><text class="title">{{storeInfo.storeName}}</text><block wx:if="{{storeInfo.isOpen}}"><image class="is-open" src="/static/welfare_icon/open_store.png" mode></image></block><block wx:else><image class="is-open" src="/static/welfare_icon/close_store.png" mode></image></block></view><view data-event-opts="{{[['tap',[['pageToChooseStore',['$event']]]]]}}" class="right" bindtap="__e"><image class="img" src="{{state.baseUrl+'/images/wxa_pic/more_store.png'}}" mode></image></view></view><view class="goods-list" style="{{'height:'+(state.statusBarHeight+'px')+';'}}"><scroll-view class="left-menu" scroll-y="true"><view class="item-box"><block wx:for="{{menuOne}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['changeIndex',[index]]]]]}}" class="{{['item',(index==activeIndex)?'active':'']}}" bindtap="__e">{{item.name}}</text></block></view></scroll-view><view class="right-list"><view class="top-swiper-box"><block wx:if="{{storeInfo.storeTip}}"><view class="notice_box"><text class="icon"></text><scroll-view class="marquee_box_container"><view class="scrolltxt"><view class="marquee_box"><view class="marquee_text" style="{{'transform:'+('translateX(-'+marqueeDistance+'rpx)')+';'}}"><text>{{storeInfo.storeTip}}</text></view></view></view></scroll-view></view></block><activity-swiper vue-id="dfd26234-2" jumpMsg="{{jumpMsg}}" autoplay="true" imgs="{{imgs}}" height="250" radius="0" bind:__l="__l"></activity-swiper></view><block wx:if="{{childArr}}"><scroll-view class="tab-box" scroll-x="true"><view class="child-tab"><block wx:for="{{childArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeChildIndex',[index]]]]]}}" class="item" bindtap="__e"><text class="{{['title',(childIndex==index)?'active':'']}}">{{item.classifyName}}</text><block wx:if="{{childIndex==index}}"><text class="bottom-line"></text></block></view></block></view></scroll-view></block><scroll-view class="goods-box" style="{{'height:'+(state.goodsHei+'px')+';'}}" scroll-y="true" data-event-opts="{{[['scrolltolower',[['onLoadMore',['$event']]]]]}}" bindscrolltolower="__e"><view class="content-box"><view class="goods-item-box"><block wx:for="{{goodsListArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods-item"><view data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsListArr','',index,'productId']]]]]]]}}" class="tag" bindtap="__e"><block wx:for="{{item.lables}}" wx:for-item="tag" wx:for-index="i" wx:key="i"><text class="tag-item">{{tag}}</text></block></view><image class="goods-img" src="{{state.baseUrl+item.picture}}" lazy-load="{{true}}" mode data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsListArr','',index,'productId']]]]]]]}}" bindtap="__e"></image><view class="parameter"><text data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsListArr','',index,'productId']]]]]]]}}" class="goods-name" bindtap="__e">{{item.goodsName}}</text><text data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsListArr','',index,'productId']]]]]]]}}" class="goods-depict" bindtap="__e">{{item.depict}}</text><view class="price"><view data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsListArr','',index,'productId']]]]]]]}}" class="left_price" bindtap="__e"><block wx:if="{{item.sellPrice}}"><text style="color:#e62329;">{{"￥"+item.sellPrice+"元"}}</text></block><block wx:if="{{item.integral}}"><text style="color:#3bb81e;">{{"+"+item.integral+"分"}}</text></block></view><view class="right_icon"><block wx:if="{{item.isTuan}}"><image src="{{state.baseUrl+'/images/wxa_pic/pin_icon.png'}}" mode data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsListArr','',index,'productId']]]]]]]}}" bindtap="__e"></image></block><block wx:if="{{item.isDiscount}}"><image src="{{state.baseUrl+'/images/wxa_pic/tuan_icon.png'}}" mode data-event-opts="{{[['tap',[['pageToGoodsDetail',['$0'],[[['goodsListArr','',index,'productId']]]]]]]}}" bindtap="__e"></image></block></view></view></view></view></block></view></view></scroll-view></view></view><view data-event-opts="{{[['tap',[['pageToShopCar',['$event']]]]]}}" class="{{['shop-car-btn',(!state.hasGoods)?'hui':'',(state.hasGoods)?'hong':'']}}" bindtap="__e"><block wx:if="{{!state.hasGoods}}"><image class="wawa" src="/static/welfare_icon/wawa_hui.png" mode></image></block><block wx:else><image class="wawa" src="/static/welfare_icon/wawa_hong.png" mode></image></block><block wx:if="{{state.hasGoods}}"><text class="count">{{goodsCount}}</text></block><view class="right-content"><text class="title">支持配送|自提</text><text class="price">{{"￥"+(goodsPrice==0?'0.00':goodsPrice)+"元"}}</text></view></view></view>